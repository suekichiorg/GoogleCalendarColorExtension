// ==UserScript==
// @name           Google Calendar Color
// @include        http://*/*
// ==/UserScript==
{
    "name": "Google Calendar Color Monday",
	"version": "1.1",
	"default_icon" : "icon.png",
	"background_page": "background.html",
	"content_scripts":[
	  {
	    "matches": ["http://www.google.com/calendar/*", "https://www.google.com/calendar/*"],
	    "css": ["content.css"],
	    "js": ["jquery.js", "content.js"]
	  }
	],
	"permissions": ["http://www.google.com/calendar/*", "https://www.google.com/calendar/*"]
}

