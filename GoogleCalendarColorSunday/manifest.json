{
    "name": "Google Calendar Color Sunday",
	"version": "1.0.4",
	"manifest_version": 2,
    "description": "Google Calendar の土日に色をつけます。日曜始まりに対応しています。",
    "icons": {
       "16": "icon_016.png",
      "128": "icon_128.png"
    },
	"content_scripts":[
	  {
	    "matches": ["https://calendar.google.com/*", "https://calendar.google.com/*"],
	    "css": ["content.css"]
	  }
	],
	"permissions": ["https://calendar.google.com/*", "https://calendar.google.com/*"]
}

